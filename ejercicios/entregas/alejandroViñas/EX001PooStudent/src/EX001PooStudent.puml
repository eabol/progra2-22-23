@startuml
Class Student{
-String name
-String surname
-String id
-float grade
+Student(name: String,surname: String,id: String)
+setGrade(grade:float)
+setName(name: String)
+setSurname(surname: String)
+setId(id: String)
+getId()
+getName()
+getSurname()
+getGrade()
}

Class Subject{
-String name
-student[] students
-boolean isOrdered
-int position
-int capacity
+Subject(name:String capacity: int)
+addStudent(student: Student)
+orderStudents()
+getMedian()
+getBestStudent()
+getWorstStudent()
}
@enduml